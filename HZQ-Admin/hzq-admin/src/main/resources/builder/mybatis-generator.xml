<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="MySql" targetRuntime="MyBatis3Simple" defaultModelType="flat">

        <!-- 注释 -->
        <commentGenerator >
            <property name="suppressAllComments" value="true"/><!-- 是否取消注释 -->
            <property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳-->
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/hzq-admin?useUnicode=true&amp;characterEncoding=utf-8"
                        userId="root"
                        password="root">
        </jdbcConnection>
        <!--生成实体-->
        <javaModelGenerator targetPackage="hzqing.com.admin.entity.system" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--生成mapper.xml文件 -->
        <sqlMapGenerator targetPackage="mapper.system"  targetProject="src/main/resources">
        </sqlMapGenerator>
        <!--生成mapper java类-->
        <javaClientGenerator type="XMLMAPPER"  targetPackage="hzqing.com.admin.mapper.system"  targetProject="src/main/java">
            <!--<property name="enableSubPackages" value="true"/>-->
            <property name="rootInterface" value="hzqing.com.common.base.mapper.BaseMapper"/>
        </javaClientGenerator>



        <table tableName="sys_log" domainObjectName="Log">
           <generatedKey column="id" identity="true" sqlStatement="MySql"/>
        </table>

    </context>
</generatorConfiguration>